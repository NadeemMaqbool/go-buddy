<main role="main" class="col-md-9 ml-sm-auto col-lg-10 pt-3 px-4 main-height" >
          <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pb-2 mb-3" >
            <div class="row col-sm-12 content-set">
              <div class="row col-sm-12 border-bottom">
                <h2 class="align-items-center"  style="align-items: center">Countries</h2>
              </div>
              <div class="col-sm-10 offset-1 mt-5">
                <table class="table table-bordered">
                  <thead>
                    <tr>
                      <th scope="col">#</th>
                      <th scope="col">Name</th>
                      <th scope="col">Short Name</th>
                      <th scope="col">Contient</th>
                      <th scope="col">Created on</th>
                      <th scope="col">Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#each countryData}}
                      <tr>
                        <th scope="row">{{@index}}</th>
                        <td>{{long_name}}</td>
                        <td>{{short_name}}</td>
                        <td>{{continent}}</td>
                        <td>{{createdAt}}</td>
                        <td>
                          <button type="button" class="btn btn-sm">Add</button>
                          <button type="button" class="btn btn-sm">Edit</button>
                          <button type="button" class="btn btn-sm">Delete</button>
                        </td>
                      </tr>
                    {{/each }}  
                  </tbody>
                </table>
            </div>
          </div>
        </main>